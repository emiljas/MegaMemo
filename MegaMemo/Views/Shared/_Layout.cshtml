<!DOCTYPE html>
@*<html lang="en" manifest="/Handlers/Manifest.ashx">*@
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - MegaMemo</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        <header>
            
            @Html.Partial("_LoginPartial")

            <canvas id="logo" width="500" height="100" />
