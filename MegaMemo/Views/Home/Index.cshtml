@{
    ViewBag.Title = "Home Page";
}

<div id="loginInfoContrainer">

</div>

<div id="loginContainer" class="formContainer">
    <label for="userName">username:</label>
    <input type="text" id="userName" data-bind="value: userName, valueUpdate: 'afterkeydown'" />
    <br />
    
    <label for="password">password:</label>
    <input type="password" id="password" data-bind="value: password, valueUpdate: 'afterkeydown'" />
    <br />
    
    <span data-bind="html: errors"></span><br />
        
    <button type="submit" id="loginBtn" data-bind="click: login">Login</button>
    <br />
</div>

